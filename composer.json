{
    "name": "lanhktc/s-cart",
    "description": "Open source e-commerce for business use laravel framework",
    "keywords": ["Open source","E-commerce","CMS", "framework", "laravel","shop", "store","business"],
    "license": "MIT",
    "authors": [
        {
            "name": "KTC",
            "email": "lanhktc@gmail.com"
        }
    ],
    "type" : "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "encore/laravel-admin": "^1.5",
        "fideloper/proxy": "^4.0",
        "gloudemans/shoppingcart": "^2.4",
        "gnugat/nomo-spaco": "~0.4",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "2.*",
        "paypal/rest-api-sdk-php": "^1.13",
        "unisharp/laravel-filemanager": "^1.9"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
